{
  "name": "nyngi",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "bun": ">=1.2.0"
  },
  "scripts": {
    "dev:host": "env-cmd -f .env.local caddy run",
    "dev:server": "FORCE_COLOR=1 env-cmd -f .env.local next dev",
    "dev": "concurrently --kill-others --names \"host,server\" -c \"yellow,cyan\" \"bun run dev:host\" \"bun run dev:server\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "oxfmt",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "PLAYWRIGHT_HTML_OPEN='never' playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "vitest run --coverage",
    "db:generate": "bun --env-file=.env.local run drizzle-kit generate",
    "db:migrate": "bun --env-file=.env.local run drizzle-kit migrate",
    "db:check": "bun --env-file=.env.local run drizzle-kit check"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.2",
    "@ai-sdk/react": "^3.0.5",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@sentry/nextjs": "^10.28.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.2",
    "@tailwindcss/cli": "^4.1.17",
    "@xyflow/react": "^12.10.0",
    "ai": "^6.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "embla-carousel-react": "^8.6.0",
    "es-toolkit": "^1.43.0",
    "lucide-react": "^0.554.0",
    "motion": "^12.23.25",
    "nanoid": "^5.1.6",
    "next": "^16.0.10",
    "next-intl": "^4.5.8",
    "next-themes": "^0.4.6",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "postgres": "^3.4.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "shiki": "^3.19.0",
    "sonner": "^2.0.7",
    "streamdown": "^1.6.10",
    "tailwind-merge": "^3.4.0",
    "tokenlens": "^1.3.1",
    "use-stick-to-bottom": "^1.1.1",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.8",
    "env-cmd": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.15.0",
    "shadcn": "^3.5.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts}": [
      "oxfmt --check",
      "eslint"
    ]
  }
}
